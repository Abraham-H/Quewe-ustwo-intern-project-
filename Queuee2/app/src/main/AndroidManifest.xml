<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.com.example.android.queuee2" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.BLUETOOTH"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".AddToQueueActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- <service android:name=".model.FirebaseService" /> -->
        <activity
            android:name=".InQueueActivity"
            android:label="@string/title_activity_in_queue" >
        </activity>
        <activity
            android:name=".ThankYouActivity"
            android:label="@string/title_activity_thank_you" >
        </activity>
        <activity
            android:name=".model.YouAreNextActivity"
            android:label="@string/title_activity_you_are_next" >
        </activity>
        <activity
            android:name=".YouAreNextActivity"
            android:label="@string/title_activity_you_are_next" >
        </activity>
        <activity
            android:name=".YouAreNext"
            android:label="@string/title_activity_you_are_next" >
        </activity>
    </application>

</manifest>
